#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/bin/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules:/mnt/e/源码文件/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/bin/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules:/mnt/e/源码文件/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
