#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/bin/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules:/mnt/e/源码文件/node_modules:/mnt/e/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/rollup-plugin-typescript2@0.31.2_rollup@2.75.6+typescript@4.7.3/node_modules/rollup-plugin-typescript2/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/rollup-plugin-typescript2@0.31.2_rollup@2.75.6+typescript@4.7.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/bin/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules/typescript/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/typescript@4.7.3/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules:/mnt/e/源码文件/node_modules:/mnt/e/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/rollup-plugin-typescript2@0.31.2_rollup@2.75.6+typescript@4.7.3/node_modules/rollup-plugin-typescript2/node_modules:/mnt/e/源码文件/ts-data-structure/node_modules/.pnpm/rollup-plugin-typescript2@0.31.2_rollup@2.75.6+typescript@4.7.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.7.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.7.3/node_modules/typescript/bin/tsserver" "$@"
fi
